/*  Основа  */
QWidget{
    background-color: rgb(40, 42, 47);
    color: rgb(250, 250, 250);
}

QLineEdit, QPlainTextEdit{
    background-color: white;
    color: rgb(5, 5, 5);
    border-radius: 5px;
}

QLabel{
    background-color: transparent;
    font: "Source Sans Pro";
}

#label{
    font: 14pt "Source Sans Pro";
}

QHeaderView::section {
    background-color: white;
    color: black;
    font-size: 12px;
    font-weight: bold;
    border: 0px solid black;
    border-bottom-right-radius: 3px;
}

QComboBox{
    background-color: rgb(250, 250, 250);
    border-radius: 10px;
    color: rgb(148, 141, 138);
}

QComboBox::drop-down {
    border-radius: 10px; /* Ширина стрелки выпадающего списка */
}

QComboBox QAbstractItemView {
    background-color: rgb(250, 250, 250);
    border-radius: 10px;
    color: rgb(255, 10, 10);
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left-width: 1px;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
    image: url(:/icons/down_arrow.png);
}

QComboBox::down-arrow:on {
    top: 1px;
    left: 1px;
}

QCheckBox
{
    color: rgb(255, 255, 255);
    background-color: transparent;
    spacing: 10px;
    font: 12pt "Source Sans Pro";
}

QCheckBox::indicator{
    width: 25px;
    height: 25px;
}
QCheckBox::indicator:unchecked {
    image: url(Icons/CheckBox_checked.png);
}

QCheckBox::indicator:checked {
    image: url(Icons/CheckBox_unchecked.png);
}


#bt_find, #bt_edit, #bt_enter{
    background-color: #8e31ff;
    color: rgb(255, 255, 255);
}

#bt_objectsc_showPhotos{
    background-color: rgb(230, 230, 230);
    color: rgb(148, 141, 138);
}

#bt_copy {
    background-color: #b431ff;
    color: white;
}

#bt_impcounts, #bt_meetcounts{
    background-color: rgb(30, 32, 37);
    color: rgb(255, 255, 255);
    font: light 10pt "Source Sans Pro";
    icon: url(Icons/RedDot.png);
    border-radius: 15px;
    text-align: right;
    padding-top: 5px; /* Устанавливаем отступ сверху */
    padding-bottom: 5px; /* Устанавливаем отступ снизу */
    padding-left: 10px; /* Устанавливаем отступ слева */
    padding-right: 10px; /* Устанавливаем отступ справа */
}

/*  Кнопка удаления     */
#bt_delete{
    background-color: #e70075;

    color: rgb(255, 255, 255);
}

/*  Выбор столбца для поиска   */
