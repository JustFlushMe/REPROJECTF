/*  Общее   */

#frame{
    background-color: rgb(30, 32, 37);
    color: rgb(245, 245, 245);
    border-radius: 25;
}

QDateTimeEdit{
    background-color: white;
    color: rgb(15, 15, 15);
    border-radius: 10px;
}

QDateTimeEdit::down-button {
    width: 20px;
    height: 20px;
    color: black;
    border-radius: 5px;
    image: url(Icons/ArrowDown.png);
}

QDateTimeEdit::up-button {
    width: 20px;
    height: 20px;
    color: black;
    border-radius: 5px;
    image: url(Icons/ArrowUp.png);
}

QSpinBox {
    color: black;
    background-color: white;
}

QSpinBox::up-button {
    background-color: white; /* Цвет фона кнопок */
    color: black; /* Цвет текста кнопок */
    image: url(Icons/ArrowUp.png);
}

QSpinBox::down-button{
    image: url(Icons/ArrowDown.png);
    background-color: white; /* Цвет фона кнопок */
    color: black; /* Цвет текста кнопок */
}

QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background-color: #2980b9; /* Цвет фона кнопок при наведении */
    border: 1px solid #2980b9; /* Граница кнопок при наведении */
}



QPushButton{
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-style: solid;
	border-bottom-width: 2px;

	border-radius: 10px;
	color: #a9b7c6;
	background-color: rgb(25, 35, 35);

}


QPushButton:hover{
	border-style: solid;

    border-top-width: 2px;
    border-right-width: 2px;
    border-left-width: 2px;
    border-bottom-width: 2px;

	border-style: solid;
	color: #FFFFFF;
}

#tableWidget_main{
    border-radius: 5px;
    alternate-background-color: #d5b3ff;
    selection-background-color: #ff00bd;
}

/* Выделение строки в tablewidget*/
#tableWidget_main::item:selected {
    color: white;
    background-color: #8e31ff;
}


